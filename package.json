{
  "name": "fastify_prizma",
  "version": "1.0.0",
  "description": "Learning prisma + fastify together",
  "main": "index.js",
  "repository": "localhost",
  "author": "techies",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"TypeScript,App\" --prefix-colors \"yellow.bold,cyan.bold\" \"nodemon\"",
    "dev:build": "tsc",
    "next:build": "next build",
    "dev:pretty": "npm run dev | pino-pretty",
    "start:prod": "cross-env NODE_ENV=production node build/index",
    "start": "node build/index",
    "clean:next": "rmdir /s .next",
    "clean:dev": "rmdir /s build",
    "clean": "npm -s run clean:dev && npm -s rum clean:next",
    "build": "npm -s rum next:build && npm -s run dev:build && npm -s run start",
    "generate:prisma": "prisma generate",
    "generate": "npm -s run generate:prisma",
    "build:complete":"npm -s rum clean && npm -s rum generate && npm -s rum build",
    "db:push": "prisma db push --preview-feature",
    "format": "npx prettier --write './**/*.{ts,md}'",
    "tap-test": "tap --reporter=list --watch",
    "test": "vitest --no-threads",
    "test-ui": "vitest --ui",
    "typecheck": "vitest typecheck",
    "coverage": "vitest run --coverage"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/jwt": "5.0.0",
    "@hookform/resolvers": "^3.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.15.1",
    "@vitest/coverage-c8": "^0.29.7",
    "@vitest/ui": "^0.29.7",
    "autoprefixer": "^10.4.14",
    "c8": "^7.13.0",
    "concurrency": "^0.1.4",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.21",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "sass": "^1.60.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.1",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.1",
    "@fastify/env": "^4.2.0",
    "@fastify/nextjs": "^9.2.0",
    "@fastify/session": "^10.1.1",
    "@fastify/static": "^6.9.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.5.0",
    "@pothos/core": "^3.28.0",
    "@pothos/plugin-prisma": "^3.43.0",
    "@pothos/plugin-scope-auth": "^3.18.0",
    "@prisma/client": "^4.11.0",
    "bcryptjs": "^2.4.3",
    "fastify": "^4.14.1",
    "fastify-caching": "^6.3.0",
    "fastify-mail": "^1.0.5",
    "fastify-plugin": "^4.5.0",
    "fastify-server-session": "^5.0.1",
    "fastify-swagger": "^5.2.0",
    "fastify-zod": "^1.2.0",
    "graphql": "^16.6.0",
    "ky": "^0.33.3",
    "ky-universal": "^0.11.0",
    "mercurius": "^12.2.0",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "swr": "^2.1.2",
    "zod": "^3.21.4"
  }
}
